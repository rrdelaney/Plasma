{
  "name": "plasma",
  "version": "0.0.0",
  "author": "Ryan Delaney <rrdelaney@outlook.com>",
  "license": "MIT",
  "scripts": {
    "dev-client": "webpack-dev-server --history-api-fallback --hot --inline --progress --colors --port 3000",
    "build-client": "cross-env NODE_ENV=production webpack -p --optimize-minimize ",
    "predev-server": "cross-env BABEL_ENV=server babel src -d _server",
    "dev-server": "cross-env BABEL_ENV=server parallelshell \"babel --watch src -d _server\" \"nodemon --watch _server _server/server.js\"",
    "build-server": "BABEL_ENV=server babel src -d target",
    "predev": "rimraf _client _server",
    "dev": "parallelshell \"npm run dev-client\" \"npm run dev-server\"",
    "prebuild": "rimraf target && mkdirp target",
    "build": "npm run build-server && npm run build-client",
    "postbuild": "cp package.json target"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^1.0.7",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.9.0",
    "parallelshell": "^2.0.0",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.4",
    "snazzy": "^3.0.0",
    "standard": "^6.0.7",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "express": "^4.13.4",
    "proxy-middleware": "^0.15.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-promise": "^0.5.1",
    "redux-reaction": "0.0.1",
    "reyle": "0.0.7"
  }
}
